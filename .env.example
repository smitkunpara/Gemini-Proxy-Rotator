# Gemini API Keys - JSON array format
GEMINI_API_KEYS='["your-api-key-1", "your-api-key-2", "your-api-key-3"]'

# Alternative: Comma-separated format
# GEMINI_API_KEYS=your-api-key-1,your-api-key-2,your-api-key-3

# Gemini API Base URL
GEMINI_API_BASE_URL=https://generativelanguage.googleapis.com

# Server Configuration
PROXY_PORT=8080
PROXY_HOST=0.0.0.0

# Logging
LOG_LEVEL=INFO

# Advanced Settings
CHUNK_SIZE=1024
STREAM_TIMEOUT=300
REQUEST_TIMEOUT=60